{
  "expo": {
    "name": "Conecta Aprendiz",
    "slug": "app-senac-aprendizagem",
    "version": "1.1.0",
    "scheme": "senac-aprendizagem",
    "orientation": "portrait",
    "icon": "./assets/images/splash_logo2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash_logo2.png",
      "resizeMode": "cover",
      "backgroundColor": "#044B8B"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash_logo2.png",
        "backgroundColor": "#b7e4c7"
      },
      "package": "com.alexdevmobile007.senacaprendizagem",
      "versionCode": 5,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo acessa suas fotos para permitir que você selecione uma imagem como avatar personalizado.",
          "cameraPermission": "O aplicativo acessa sua câmera para permitir que você tire uma foto como avatar personalizado."
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "20359a4a-3090-4d9b-9d97-29e9b1e97d30"
      }
    },
    "owner": "alexdevmobile007"
  }
}